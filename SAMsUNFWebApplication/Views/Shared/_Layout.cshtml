<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title-Student Action Management System</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="shortcut icon" href="@Url.Content("~/Content/SiteTemplate/")favicon.ico">
    <!-- Global CSS -->
    <link rel="stylesheet" href="@Url.Content("~/Content/SiteTemplate/")plugins/bootstrap/css/bootstrap.min.css">
    <!-- Plugins CSS -->
    <link rel="stylesheet" href="@Url.Content("~/Content/SiteTemplate/")plugins/font-awesome/css/font-awesome.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/SiteTemplate/")plugins/flexslider/flexslider.css">
    <!-- Theme CSS -->
    <link id="theme-style" rel="stylesheet" href="@Url.Content("~/Content/SiteTemplate/")css/styles.css">
    <link href="@Url.Content("~/Content/Gridmvc.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/SAMLayout.css")" rel="stylesheet" type="text/css" />

    <script type="text/javascript" src="@Url.Content("~/Content/SiteTemplate/")plugins/jquery-1.11.2.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/SiteTemplate/")plugins/jquery-migrate-1.2.1.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/SiteTemplate/")plugins/bootstrap/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/SiteTemplate/")plugins/bootstrap-hover-dropdown.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/SiteTemplate/")plugins/back-to-top.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/SiteTemplate/")plugins/jquery-placeholder/jquery.placeholder.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/SiteTemplate/")plugins/FitVids/jquery.fitvids.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/SiteTemplate/")plugins/flexslider/jquery.flexslider-min.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/SiteTemplate/")js/main.js"></script>
    <script src="@Url.Content("~/Scripts/gridmvc.min.js")" type="text/javascript"> </script>

</head>
<body>
    <!-- ******HEADER****** -->
    <div class="container container-table">
            <div class="row vertical-center-row">
                    <nav class="navbar  navbar-fixed-top navbar-custom">
                        <div id="navbar" class="navbar">
                            <ul class="nav navbar-nav">
                                <li class="nav-item" ><a id="img-action" href="@Url.Action("Index", "Home")"><img src="~/Content/SiteTemplate/images/background/JaxLogo.jpg" /></a></li>
                                <li class="nav-item">@Html.ActionLink("Dashboard", "Dashboard", "Dashboard")</li>
                                <li class="nav-item">@Html.ActionLink("Office Visit", "OfficeVisit", "OfficeVisit")</li>
                                <li class="nav-item dropdown">
                                    <a class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-delay="0" data-close-others="false" href="#">Admin Management <i class="fa fa-angle-down"></i></a>
                                    <ul class="dropdown-menu">
                                        <li>@Html.ActionLink("Student", "Student", "Student")</li>
                                        <li>@Html.ActionLink("School", "School", "School")</li>
                                        <li>@Html.ActionLink("Homeroom", "Homeroom", "Homeroom")</li>
                                        <li>@Html.ActionLink("Contact", "Contact", "Contact")</li>
										<li>@Html.ActionLink("Profile", "Profile", "Profile")</li>
                                        <li>@Html.ActionLink("Code of Conduct", "CodeOfConductViolation", "CodeOfConductViolation")</li>
                                        <li>@Html.ActionLink("Remedial Action", "RemedialAction", "RemedialAction")</li>
                                        <li class="nav-divider"></li>
                                        <li>@Html.ActionLink("Import Data", "Import", "Import")</li>
                                        <li class="nav-divider"></li>
                                        <li>@Html.ActionLink("School Year", "SchoolYear", "SchoolYear")</li>
                                    </ul>
                                </li><!--//dropdown-->
                                <li class="nav-item dropdown">
                                    <a class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-delay="0" data-close-others="false" href="#">
                                        Training
                                        <i class="fa fa-angle-down"></i>
                                    </a>
                                    <ul class="dropdown-menu">
                                        <li>@Html.ActionLink("Administrator Training", "Training", "Training")</li>
                                        <li>@Html.ActionLink("Technical Training", "Training", "Training")</li>
                                    </ul>
                                </li><!--//dropdown-->
                                <li class="nav-item">@Html.ActionLink("Login", "Login", "Login")</li>
                            </ul><!--//nav-->
                        </div>
                   </nav>
</div>
            <div class="row vertical-center-row">
                @RenderBody()
            </div>
            <div class="row vertical-center-row">
                <p class="copyright">Heptad Assembly And University oF North Florida CCEC - 2016</p>
            </div>
    </div>
</body>
</html>